# StylusTx Environment Configuration
# Copy this to .env and fill in your values
# NEVER commit .env to git!

# ===========================================
# DEPLOYMENT CONFIGURATION
# ===========================================

# Private key for deploying contracts (use testnet wallet only!)
DEPLOYER_PRIVATE_KEY=0x_your_deployer_private_key_here

# RPC URL for Arbitrum Sepolia testnet
RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# ===========================================
# DEPLOYED CONTRACT ADDRESSES
# ===========================================

# Deployed paymaster contract address (fill after deployment)
PAYMASTER_ADDRESS=0x_paymaster_address_after_deployment

# Target contract address (the contract that can be called gaslessly)
TARGET_CONTRACT_ADDRESS=0x_target_contract_address

# ===========================================
# RELAYER CONFIGURATION
# ===========================================

# Private key for relayer wallet (pays gas fees)
# WARNING: TESTNET ONLY! Never use mainnet keys here!
RELAYER_PRIVATE_KEY=0x_your_relayer_private_key_here

# ===========================================
# OPTIONAL CONFIGURATION
# ===========================================

# Chain ID (421614 for Arbitrum Sepolia)
CHAIN_ID=421614

# Block explorer URL
EXPLORER_URL=https://sepolia.arbiscan.io
